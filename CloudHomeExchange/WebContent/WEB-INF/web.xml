    <servlet-name>HouseSearchServlet</servlet-name>
    <url-pattern>/searchHouse</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HouseAddServlet</servlet-name>
    <servlet-class>com.servlet.HouseAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HouseAddServlet</servlet-name>
    <url-pattern>/addHouse</url-pattern>
