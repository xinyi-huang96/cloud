    <servlet-class>com.servlet.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserLoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UserUpdateInfoServlet</servlet-name>
    <servlet-class>com.servlet.UserUpdateInfoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserUpdateInfoServlet</servlet-name>
    <url-pattern>/updateUser</url-pattern>
  </servlet-mapping>
