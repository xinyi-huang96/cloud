    <servlet-name>OrderCreateServlet</servlet-name>
    <url-pattern>/createOrder</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>OrderProcessServlet</servlet-name>
    <servlet-class>com.servlet.OrderProcessServlet</servlet-class>
  </servlet>
